{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "target": "ESNext",
    "downlevelIteration": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": { },
    "jsxImportSource": "nativewind"
  },
  "include": [
    "scripts/JIRA/**/*.ts",
    "scripts/JIRA/**/*.tsx",
  ],
  "exclude": [
    "node_modules",
    "node_modules/*"
  ]
}
