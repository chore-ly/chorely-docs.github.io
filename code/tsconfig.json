{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "CommonJS",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "target": "ESNext",
    "downlevelIteration": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": { },
    "jsxImportSource": "nativewind",
    "types": [
      "obsidian-typings"
    ]
  },
  "include": [
    "./dataview/**/*/*.ts",
    "./scripts/**/*/*.ts",
  ],
  "exclude": [
    
    "node_modules",
    "node_modules/*"
  ]
}
